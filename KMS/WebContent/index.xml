    	<script type="text/javascript" lazy="false"><![CDATA[
        
	scwin.onpageload = function() {
		scwin.setMenu();
	};
	
	scwin.onpageunload = function() {
		
	};
	
	
	scwin.setMenu = function(){
		genMenu.removeAll();
		
		var count = dc_menu.getRowCount();
		for(var i=0; i<count; i++){
			genMenu.insertChild(i);
			genMenu.getChild(i, "ancMenu").setValue(dc_menu.getCellData(i, "menu"));
			genMenu.getChild(i, "ancMenu").setTitle(dc_menu.getCellData(i, "menu"));
		}
	};
	
	scwin.ancMenu_onclick = function() {
		tab1.deleteAllTabs();
		
		var idx = this.getGeneratedIndex();
		var _tabKey = "pdf_"+ dc_menu.getCellData(idx, "no");
		var _url = dc_menu.getCellData(idx, "url") + dc_menu.getCellData(idx, "menu") + ".pdf";
		var _label = dc_menu.getCellData(idx, "menu");
		//iframe1.setSrc(_url);
		
		var tabOptions = {
			"label" : "문서",
			"closable" : true,
			"openAction" : "select"
		};
		
		var contentsOptions = {
			"src" : _url,
			"frameMode" : "iframe"
		};
		
		tab1.addTab(_tabKey, tabOptions, contentsOptions);
		
		// sample page 화면 실행 
		var _sample = dc_menu.getCellData(idx, "sample");
		var _sampleObj = _sample.split(",");
		var _sampleCnt = _sampleObj.length;
		
		for(var j=0; j<_sampleCnt; j++){
			
			var _sampleKey = "ex"+j;
			var _sampleName = _sampleObj[j];
			var _sampleUrl = dc_menu.getCellData(idx, "url") + _sampleObj[j];
			
			var tabOpt = {
				"label" : _sampleName,
				"closable" : true,
				"openAction" : "select"
			};
			
			var contentsOpt = {
				"src" : _sampleUrl,
				"frameMode" : "wframePreload"  
			};
			
			if(!_sampleObj[j]){
			
			}else{
				tab1.addTab(_sampleKey, tabOpt, contentsOpt);
			}
		}
		
		//선택한 tab 활성화.
		tab1.setSelectedTabIndex(0);
		
	};
	
	]]></script>
